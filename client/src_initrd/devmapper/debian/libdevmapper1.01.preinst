#! /bin/sh

set -e

case "$1" in
    install|upgrade)
	dpkg --compare-versions "$2" '<<' '2:1.01.00-2' && update-rc.d -f libdevmapper1.01 remove > /dev/null
    ;;

    abort-upgrade)
    ;;

    *)
        echo "preinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0


